# 1Password Template for traefik
# This file contains references to secrets stored in 1Password
# Run 'op inject -i .env.template -o .env.local' to generate real .env file

# Domain configuration
DOMAIN=op://Docker-Stack/Traefik-Domain/password

# ACME/Let's Encrypt
ACME_EMAIL=op://Docker-Stack/Traefik-Acme-Email/password

# Traefik authentication
TRAEFIK_AUTH=op://Docker-Stack/Traefik-Auth/password

# Logging
LOG_LEVEL=INFO

# Network
TRAEFIK_NETWORK=traefik
